--// ================= FUN√á√ïES UI =================
local function getPlayers()
	local t = {}
	for _, p in ipairs(Players:GetPlayers()) do
		table.insert(t,p.Name)
	end
	return t
end

local function dropdown(sec, cb)
	return sec:Dropdown({
		Title="Selecionar Jogador",
		Values=getPlayers(),
		Callback=cb
	})
end

--// ================= ABA COMANDOS COM √çCONES √öNICOS =================
do
	local tab = Window:Tab({Title="Comandos", Icon="rbxassetid://10709781460"})
	local sec = tab:Section({Title="A√ß√µes de Controle", Opened=true})

	local alvo
	dropdown(sec,function(v) alvo=v end)

	sec:Button({
		Title="Verificar",
		Desc="Verifica quem est√° com o c√≥digo ativo",
		Icon="rbxassetid://10734950309",
		Callback=function()
			EnviarComando("verifique","")
		end
	})

	sec:Button({
		Title="Kick",
		Desc="Remove o jogador do servidor",
		Icon="rbxassetid://10747384394",
		Callback=function()
			if alvo then 
				EnviarComando("kick", alvo)
				Window:Notify({
					Title = "Comando Enviado",
					Content = "Kick enviado para: "..alvo,
					Duration = 3
				})
			else
				Window:Notify({
					Title = "Erro",
					Content = "Selecione um jogador primeiro!",
					Duration = 3
				})
			end
		end
	})

	for _, v in ipairs({
		{cmd="kill", desc="Mata o jogador", icon="rbxassetid://10709790644"},
		{cmd="bring", desc="Traz o jogador at√© voc√™", icon="rbxassetid://10734884548"},
		{cmd="explode", desc="Explode o jogador", icon="rbxassetid://10723434711"},
		{cmd="fling", desc="Arremessa o jogador", icon="rbxassetid://10709770005"},
		{cmd="jail", desc="Prende o jogador", icon="rbxassetid://10723345518"},
		{cmd="unjail", desc="Solta o jogador da pris√£o", icon="rbxassetid://10723346959"},
		{cmd="blind", desc="Tela preta", icon="rbxassetid://10709782497"},
		{cmd="unblind", desc="Remove tela preta", icon="rbxassetid://10723407389"},
	}) do
		sec:Button({
			Title=v.cmd:gsub("^%l", string.upper),
			Desc=v.desc,
			Icon=v.icon,
			Callback=function()
				if alvo then 
					EnviarComando(v.cmd:lower(), alvo)
					Window:Notify({
						Title = "Comando Enviado",
						Content = v.cmd:gsub("^%l", string.upper) .. " enviado para: " .. alvo,
						Duration = 3
					})
				else
					Window:Notify({
						Title = "Erro",
						Content = "Selecione um jogador primeiro!",
						Duration = 3
					})
				end
			end
		})
	end
end
