--// ================= TAG PERMANENTE =================
local function criarTag(player)
	if not Autorizados[player.Name] then return end
	if not player.Character then return end

	local function aplicar()
		if not player.Character then return end
		if player.Character:FindFirstChild("EmbeeTag") then return end

		local head = player.Character:FindFirstChild("Head")
		if not head then return end

		local gui = Instance.new("BillboardGui")
		gui.Name = "EmbeeTag"
		gui.Size = UDim2.new(0, 150, 0, 28)
		gui.StudsOffset = Vector3.new(0, 3, 0)
		gui.AlwaysOnTop = true
		gui.Adornee = head

		local txt = Instance.new("TextLabel")
		txt.Size = UDim2.fromScale(1, 1)
		txt.BackgroundTransparency = 1
		txt.Text = "[" .. Autorizados[player.Name] .. "]"
		txt.Font = Enum.Font.GothamBold
		txt.TextScaled = true
		txt.TextColor3 = Color3.fromRGB(255, 90, 0)
		txt.TextStrokeTransparency = 0.3
		txt.Parent = gui

		gui.Parent = player.Character
	end

	aplicar()

	-- Reaplica se morrer / respawnar
	local hum = player.Character:FindFirstChildOfClass("Humanoid")
	if hum then
		hum.Died:Connect(function()
			task.delay(1, aplicar)
		end)
	end

	-- Se a tag for removida manualmente, recria
	player.Character.ChildRemoved:Connect(function(child)
		if child.Name == "EmbeeTag" then
			task.delay(0.2, aplicar)
		end
	end)
end

-- Aplica para jogadores atuais
for _, player in ipairs(Players:GetPlayers()) do
	player.CharacterAdded:Connect(function()
		task.wait(0.5)
		criarTag(player)
	end)
	if player.Character then
		criarTag(player)
	end
end

-- Aplica para novos jogadores
Players.PlayerAdded:Connect(function(player)
	player.CharacterAdded:Connect(function()
		task.wait(0.5)
		criarTag(player)
	end)
end)

--// ================= ENVIAR COMANDO =================
local function EnviarComando(cmd, alvo)
	local canal = TextChatService.TextChannels:FindFirstChild("RBXGeneral")
		or TextChatService.TextChannels:GetChildren()[1]

	if not canal then return end

	if alvo and alvo ~= "" then
		canal:SendAsync("//EMBEE_CMD//"..cmd.." "..alvo)
	else
		canal:SendAsync("//EMBEE_CMD//"..cmd)
	end
end

--// ================= EXECUTOR =================
local function ExecutarComando(cmd, remetente)
	local char = LocalPlayer.Character
	local hum = char and char:FindFirstChildOfClass("Humanoid")
	local root = char and char:FindFirstChild("HumanoidRootPart")
	if not char then return end

	-- Sistema de kick
	if cmd == "kick" then
		LocalPlayer:Kick("üî• Voc√™ foi removido por um administrador Embee Hub.")
		return
	end

	if cmd == "kill" then
		char:BreakJoints()

	elseif cmd == "respawn" then
		LocalPlayer:LoadCharacter()

	elseif cmd == "bring" and root then
		-- Teleporta para a posi√ß√£o de quem enviou o comando
		local sender = Players:FindFirstChild(remetente)
		if sender and sender.Character and sender.Character:FindFirstChild("HumanoidRootPart") then
			root.CFrame = sender.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3)
		end

	elseif cmd == "freeze" and hum then
		hum.WalkSpeed = 0

	elseif cmd == "unfreeze" and hum then
		hum.WalkSpeed = 16

	elseif cmd == "speed" and hum then
		hum.WalkSpeed = 50

	elseif cmd == "ws" and hum then
		hum.WalkSpeed = 25

	elseif cmd == "jp" and hum then
		hum.JumpPower = 100

	elseif cmd == "sit" and hum then
		hum.Sit = true

	elseif cmd == "jump" and hum then
		hum.Jump = true

	elseif cmd == "god" and hum then
		hum.MaxHealth = math.huge
		hum.Health = math.huge

	elseif cmd == "ungod" and hum then
		hum.MaxHealth = 100
		hum.Health = 100

	elseif cmd == "explode" and root then
		local e = Instance.new("Explosion", workspace)
		e.Position = root.Position

	elseif cmd == "fling" and root then
		root.CFrame = CFrame.new(0,-500,0)
		local bv = Instance.new("BodyVelocity", root)
		bv.Velocity = Vector3.new(0,1000,0)
		bv.MaxForce = Vector3.new(1e9,1e9,1e9)
		task.delay(0.5,function() bv:Destroy() end)

	elseif cmd == "blind" then
		local g = Instance.new("ScreenGui", LocalPlayer.PlayerGui)
		g.Name = "Blind"
		local f = Instance.new("Frame", g)
		f.Size = UDim2.fromScale(1,1)
		f.BackgroundColor3 = Color3.new(0,0,0)

	elseif cmd == "unblind" then
		local g = LocalPlayer.PlayerGui:FindFirstChild("Blind")
		if g then g:Destroy() end

	elseif cmd == "jail" and root then
		-- Marca o jogador como preso
		if not _G.EmbeePrisioneiros then _G.EmbeePrisioneiros = {} end
		_G.EmbeePrisioneiros[LocalPlayer.Name] = true
		
		-- Fun√ß√£o para criar a pris√£o
		local function criarPrisao()
			local char = LocalPlayer.Character
			if not char then return end
			local rootPart = char:FindFirstChild("HumanoidRootPart")
			if not rootPart then return end
			
			-- Remove pris√£o antiga se existir
			local oldJail = workspace:FindFirstChild("EmbeeJail_"..LocalPlayer.Name)
			if oldJail then oldJail:Destroy() end
			
			local jail = Instance.new("Model", workspace)
			jail.Name = "EmbeeJail_"..LocalPlayer.Name
			
			local size = 12
			local jailCenter = rootPart.Position
			
			-- Salva a posi√ß√£o central da pris√£o
			_G.EmbeeJailPosition = _G.EmbeeJailPosition or {}
			_G.EmbeeJailPosition[LocalPlayer.Name] = jailCenter
			
			-- Criar as paredes da pris√£o com gradiente vermelho escuro
			for i = 1, 4 do
				local wall = Instance.new("Part", jail)
				wall.Size = Vector3.new(size, size, 0.5)
				wall.Anchored = true
				wall.CanCollide = true
				wall.Material = Enum.Material.ForceField
				wall.Color = Color3.fromRGB(139, 0, 0)
				wall.Transparency = 0.2
				wall.CastShadow = false
				
				-- Adiciona brilho vermelho nas paredes
				local surfaceLight = Instance.new("SurfaceLight", wall)
				surfaceLight.Color = Color3.fromRGB(255, 0, 0)
				surfaceLight.Brightness = 2
				surfaceLight.Range = 20
				
				if i == 1 then
					wall.CFrame = CFrame.new(jailCenter.X, jailCenter.Y, jailCenter.Z + size/2)
					surfaceLight.Face = Enum.NormalId.Back
				elseif i == 2 then
					wall.CFrame = CFrame.new(jailCenter.X, jailCenter.Y, jailCenter.Z - size/2)
					surfaceLight.Face = Enum.NormalId.Front
				elseif i == 3 then
					wall.CFrame = CFrame.new(jailCenter.X + size/2, jailCenter.Y, jailCenter.Z) * CFrame.Angles(0, math.rad(90), 0)
					surfaceLight.Face = Enum.NormalId.Back
				else
					wall.CFrame = CFrame.new(jailCenter.X - size/2, jailCenter.Y, jailCenter.Z) * CFrame.Angles(0, math.rad(90), 0)
					surfaceLight.Face = Enum.NormalId.Front
				end
				
				-- Adiciona grades nas paredes
				for j = 1, 8 do
					local bar = Instance.new("Part", jail)
					bar.Size = Vector3.new(0.3, size - 1, 0.3)
					bar.Anchored = true
					bar.CanCollide = false
					bar.Material = Enum.Material.Neon
					bar.Color = Color3.fromRGB(255, 0, 0)
					bar.CastShadow = false
					
					local offset = (j - 4.5) * (size / 9)
					if i <= 2 then
						bar.CFrame = wall.CFrame * CFrame.new(offset, 0, 0.3)
					else
						bar.CFrame = wall.CFrame * CFrame.new(offset, 0, 0.3)
					end
				end
			end
			
			-- Teto com efeito de grade vermelha brilhante
			local roof = Instance.new("Part", jail)
			roof.Size = Vector3.new(size, 0.5, size)
			roof.CFrame = CFrame.new(jailCenter.X, jailCenter.Y + size/2, jailCenter.Z)
			roof.Anchored = true
			roof.CanCollide = true
			roof.Material = Enum.Material.Glass
			roof.Color = Color3.fromRGB(139, 0, 0)
			roof.Transparency = 0.3
			roof.CastShadow = false
			
			-- Luz no teto
			local roofLight = Instance.new("PointLight", roof)
			roofLight.Color = Color3.fromRGB(255, 0, 0)
			roofLight.Brightness = 3
			roofLight.Range = 25
			
			-- Ch√£o com padr√£o xadrez vermelho e preto
			for x = -1, 1 do
				for z = -1, 1 do
					local floor = Instance.new("Part", jail)
					floor.Size = Vector3.new(size/3, 0.3, size/3)
					floor.CFrame = CFrame.new(
						jailCenter.X + (x * size/3),
						jailCenter.Y - size/2,
						jailCenter.Z + (z * size/3)
					)
					floor.Anchored = true
					floor.CanCollide = true
					floor.Material = Enum.Material.Marble
					
					-- Padr√£o xadrez
					if (x + z) % 2 == 0 then
						floor.Color = Color3.fromRGB(80, 0, 0)
					else
						floor.Color = Color3.fromRGB(20, 0, 0)
					end
					floor.CastShadow = false
				end
			end
			
			-- Adicionar efeito de fuma√ßa vermelha
			local smoke = Instance.new("ParticleEmitter", roof)
			smoke.Texture = "rbxasset://textures/particles/smoke_main.dds"
			smoke.Color = ColorSequence.new(Color3.fromRGB(255, 0, 0))
			smoke.Rate = 15
			smoke.Lifetime = NumberRange.new(2, 4)
			smoke.Speed = NumberRange.new(1, 2)
			smoke.SpreadAngle = Vector2.new(180, 180)
			smoke.Size = NumberSequence.new({
				NumberSequenceKeypoint.new(0, 0),
				NumberSequenceKeypoint.new(0.5, 2),
				NumberSequenceKeypoint.new(1, 0)
			})
			smoke.Transparency = NumberSequence.new({
				NumberSequenceKeypoint.new(0, 1),
				NumberSequenceKeypoint.new(0.3, 0.5),
				NumberSequenceKeypoint.new(1, 1)
			})
			smoke.Rotation = NumberRange.new(0, 360)
			smoke.RotSpeed = NumberRange.new(-50, 50)
			
			-- Part√≠culas de fogo ao redor
			for i = 1, 4 do
				local firepart = Instance.new("Part", jail)
				firepart.Size = Vector3.new(1, 1, 1)
				firepart.Transparency = 1
				firepart.Anchored = true
				firepart.CanCollide = false
				
				local angle = (i - 1) * (math.pi / 2)
				firepart.CFrame = CFrame.new(
					jailCenter.X + math.cos(angle) * (size/2 - 1),
					jailCenter.Y - size/4,
					jailCenter.Z + math.sin(angle) * (size/2 - 1)
				)
				
				local fire = Instance.new("ParticleEmitter", firepart)
				fire.Texture = "rbxasset://textures/particles/sparkles_main.dds"
				fire.Color = ColorSequence.new({
					ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 50, 0)),
					ColorSequenceKeypoint.new(0.5, Color3.fromRGB(255, 0, 0)),
					ColorSequenceKeypoint.new(1, Color3.fromRGB(139, 0, 0))
				})
				fire.Rate = 30
				fire.Lifetime = NumberRange.new(1, 2)
				fire.Speed = NumberRange.new(3, 5)
				fire.Acceleration = Vector3.new(0, 2, 0)
				fire.SpreadAngle = Vector2.new(30, 30)
				fire.Size = NumberSequence.new({
					NumberSequenceKeypoint.new(0, 0.5),
					NumberSequenceKeypoint.new(0.5, 0.8),
					NumberSequenceKeypoint.new(1, 0)
				})
				fire.Transparency = NumberSequence.new({
					NumberSequenceKeypoint.new(0, 0.3),
					NumberSequenceKeypoint.new(1, 1)
				})
				fire.LightEmission = 1
			end
			
			-- Texto "PRESO" acima da pris√£o
			local billboard = Instance.new("Part", jail)
			billboard.Size = Vector3.new(8, 0.1, 4)
			billboard.CFrame = CFrame.new(jailCenter.X, jailCenter.Y + size/2 + 3, jailCenter.Z)
			billboard.Anchored = true
			billboard.CanCollide = false
			billboard.Transparency = 1
			
			local gui = Instance.new("SurfaceGui", billboard)
			gui.Face = Enum.NormalId.Front
			gui.AlwaysOnTop = true
			
			local text = Instance.new("TextLabel", gui)
			text.Size = UDim2.fromScale(1, 1)
			text.BackgroundTransparency = 1
			text.Text = "üî• PRESO üî•"
			text.Font = Enum.Font.GothamBold
			text.TextScaled = true
			text.TextColor3 = Color3.fromRGB(255, 0, 0)
			text.TextStrokeTransparency = 0
			text.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
			
			-- Anima√ß√£o do texto
			local TweenService = game:GetService("TweenService")
			local tween = TweenService:Create(
				text,
				TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true),
				{TextTransparency = 0.3}
			)
			tween:Play()
		end
		
		-- Cria a pris√£o inicial
		criarPrisao()
		
		-- Sistema anti-escape (teleporte, fly, noclip)
		if not _G.EmbeeAntiEscape then
			_G.EmbeeAntiEscape = game:GetService("RunService").Heartbeat:Connect(function()
				if _G.EmbeePrisioneiros and _G.EmbeePrisioneiros[LocalPlayer.Name] then
					local char = LocalPlayer.Character
					if char then
						local rootPart = char:FindFirstChild("HumanoidRootPart")
						local humanoid = char:FindFirstChildOfClass("Humanoid")
						if rootPart and _G.EmbeeJailPosition[LocalPlayer.Name] then
							local jailPos = _G.EmbeeJailPosition[LocalPlayer.Name]
							local currentPos = rootPart.Position
							local distance = (currentPos - jailPos).Magnitude
							
							-- Se o jogador se mover QUALQUER dist√¢ncia do centro, teleporta de volta
							if distance > 0.5 then
								rootPart.CFrame = CFrame.new(jailPos)
								rootPart.Velocity = Vector3.new(0, 0, 0)
								rootPart.RotVelocity = Vector3.new(0, 0, 0)
								
								-- Paralisa o movimento
								if humanoid then
									humanoid.WalkSpeed = 0
									humanoid.JumpPower = 0
									humanoid.JumpHeight = 0
								end
								
								-- Remove qualquer BodyMover que possa estar sendo usado (fly)
								for _, obj in pairs(rootPart:GetChildren()) do
									if obj:IsA("BodyMover") or obj:IsA("BodyVelocity") or obj:IsA("BodyPosition") or obj:IsA("BodyGyro") then
										obj:Destroy()
									end
								end
								
								-- Remove qualquer script de movimento
								for _, obj in pairs(char:GetDescendants()) do
									if obj:IsA("BodyMover") then
										obj:Destroy()
									end
								end
							end
						end
					end
				end
			end)
		end
		
		-- Monitora respawn para recriar a pris√£o
		if not _G.EmbeeJailConnection then
			_G.EmbeeJailConnection = LocalPlayer.CharacterAdded:Connect(function()
				task.wait(0.5)
				if _G.EmbeePrisioneiros and _G.EmbeePrisioneiros[LocalPlayer.Name] then
					criarPrisao()
				end
			end)
		end

	elseif cmd == "unjail" then
		-- Remove a marca√ß√£o de prisioneiro
		if _G.EmbeePrisioneiros then
			_G.EmbeePrisioneiros[LocalPlayer.Name] = nil
		end
		
		-- Remove a posi√ß√£o salva
		if _G.EmbeeJailPosition then
			_G.EmbeeJailPosition[LocalPlayer.Name] = nil
		end
		
		-- Remove a pris√£o
		local jail = workspace:FindFirstChild("EmbeeJail_"..LocalPlayer.Name)
		if jail then
			jail:Destroy()
		end
		
		-- Restaura os movimentos normais
		local char = LocalPlayer.Character
		if char then
			local humanoid = char:FindFirstChildOfClass("Humanoid")
			if humanoid then
				humanoid.WalkSpeed = 16
				humanoid.JumpPower = 50
				humanoid.JumpHeight = 7.2
			end
		end
	end
end

--// ================= CHAT LISTENER =================
local function OnMessage(msg)
	if not msg.Text or not msg.TextSource then return end
	if not Autorizados[msg.TextSource.Name] then return end
	if not msg.Text:match("^//EMBEE_CMD//") then return end

	local cmd, alvo = msg.Text:match("//EMBEE_CMD//(%S+)%s*(%S*)")
	if not cmd then return end
	
	cmd = cmd:lower()
	alvo = alvo and alvo:lower() or ""

	-- Comando de verifica√ß√£o sempre responde
	if cmd == "verifique" then
		local canal = TextChatService.TextChannels:FindFirstChild("RBXGeneral")
			or TextChatService.TextChannels:GetChildren()[1]
		if canal then
			canal:SendAsync("üî• Embee_Cliente_ON | "..LocalPlayer.Name)
		end
		return
	end

	-- Executa comando em TODOS que tem o c√≥digo (incluindo autorizados)
	-- Se tem alvo especificado, executa apenas nele
	-- Se n√£o tem alvo, executa em todos
	if alvo ~= "" then
		if alvo == LocalPlayer.Name:lower() then
			ExecutarComando(cmd, msg.TextSource.Name)
		end
	else
		-- Executa em todos que tem o c√≥digo (sem exce√ß√µes)
		ExecutarComando(cmd, msg.TextSource.Name)
	end
end

for _, ch in ipairs(TextChatService.TextChannels:GetChildren()) do
	if ch:IsA("TextChannel") then
		ch.MessageReceived:Connect(OnMessage)
	end
end

